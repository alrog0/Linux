grep -ir "NoDisplay=true" /etc/xdg/autostart/*.desktop
tar cvfz /root/etc-xdg-autostart.tar.gz /etc/xdg/autostart/*.desktop
sed -i 's/NoDisplay=true/NoDisplay=false/g' /etc/xdg/autostart/*.desktop
